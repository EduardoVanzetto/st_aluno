<p id="notice"><%= notice %></p>

<h1>Nota Trabalhos</h1>

<table>
  <thead>
    <tr>
      <th>Curso</th>
      <th>Disciplina</th>
      <th>Nota</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @nota_trabalhos.each do |nota_trabalho| %>
      <tr>
        <td><%= nota_trabalho.curso %></td>
        <td><%= nota_trabalho.disciplina %></td>
        <td><%= nota_trabalho.nota %></td>
        <td><%= link_to 'Show', nota_trabalho %></td>
        <td><%= link_to 'Edit', edit_nota_trabalho_path(nota_trabalho) %></td>
        <td><%= link_to 'Destroy', nota_trabalho, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Nota Trabalho', new_nota_trabalho_path %>
